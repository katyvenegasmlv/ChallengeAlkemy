\n  peticionDelete = () => {\r\n    axios.delete(\"http://localhost:8080/user/DeleteUser/\" + this.state.form.id_users).then(response => {\r\n      this.setState({ modalEliminar: false });\r\n      this.peticionGet();\r\n    })\r\n  }\r\n\r\n  modalInsertar = () => {\r\n    this.setState({ modalInsertar: !this.state.modalInsertar });\r\n  }\r\n\r\n  seleccionarUser = (users) => {\r\n    this.setState({\r\n      tipoModal: 'actualizar',\r\n      form: {\r\n        id_users: users.id_users,\r\n        user_name: users.user_name,\r\n        password: '',\r\n        email: users.email,\r\n        last_name: users.last_name,\r\n        role: users.role\r\n      }\r\n    })\r\n  }\r\n\r\n  handleChange = async e => {\r\n    e.persist();\r\n    await this.setState({\r\n      form: {\r\n        ...this.state.form,\r\n        [e.target.name]: e.target.value\r\n      }\r\n    });\r\n    console.log(this.state.form);\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.peticionGet();\r\n  }\r\n\r\n\r\n  render() {\r\n    const { form } = this.state;\r\n    return (\r\n      \r\n      <div className=\"App\">\r\n        <button className=\"btn btn-success\" onClick={() => { this.setState({ form: null, tipoModal: 'insertar' }); this.modalInsertar() }}>Crear Usuario</button>\r\n      \r\n        <table class=\"table\"  id=\"table-user\">\r\n          <thead class=\"thead-light\">\r\n            <tr>\r\n              <th>ID</th>\r\n              <th>User</th>\r\n              <th>Email</th>\r\n              <th>Last Name</th>\r\n              <th>Role</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            {this.state.data.map(users => {\r\n              return (\r\n                <tr>\r\n                  <td>{users.id_users}</td>\r\n                  <td>{users.user_name}</td>\r\n                  <td>{users.email}</td>\r\n                  <td>{users.last_name}</td>\r\n                  <td>{users.role}</td>\r\n        